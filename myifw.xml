<rules>
  <activity block="true" log="true">
    <!-- prevent third party website ad from invoking taobao app -->
    <intent-filter>
      <action name="android.intent.action.VIEW" />
      <scheme name="taobao" />
    </intent-filter>
    
    <!-- prevent m.taobao.com from invoking taobao app -->
    <intent-filter>
      <action name="android.intent.action.VIEW" />
      <scheme name="tbopen" />
    </intent-filter>
    
    <!-- prevent m.jd.com from invoking jd app -->
    <intent-filter>
      <action name="android.intent.action.VIEW" />
      <scheme name="openapp.jdmobile" />
    </intent-filter>
  </activity>
</rules>
